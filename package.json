{
  "name": "rhtmlLabelUtils",
  "private": "true",
  "version": "1.0.2",
  "description": "label utilities and shared label components for rhtml packages",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "rm -r dist/; mkdir -p dist; babel --ignore node_modules,dist,examples ./src/ -d dist",
    "compile_internal": "browserify -d test/utils/addLabelUtilsToWindow.js -o browser/js/addLabelUtilsToWindow.js -t [ babelify --presets [ @babel/preset-env ] ]",
    "serve": "rm -rf browser && mkdir -p browser && cp -r internal_www/ browser && cp -r node_modules/d3/dist/d3.js browser/external && npm run compile_internal && serve browser -l 9000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Displayr/rhtmlLabelUtils.git"
  },
  "author": "Kyle Zeeuwen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Displayr/rhtmlLabelUtils/issues"
  },
  "homepage": "https://github.com/Displayr/rhtmlLabelUtils#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "core-js": "^3.7.0",
    "d3": "^5.16.0",
    "jest": "^26.6.3",
    "jest-image-snapshot": "^4.2.0",
    "lodash": "^4.17.15",
    "puppeteer": "^5.5.0",
    "regenerator-runtime": "^0.13.7",
    "serve": "^11.3.2"
  },
  "dependencies": {}
}
